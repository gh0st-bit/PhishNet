{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "==== ORIGINAL SCRIPTS ====": "",
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:win": "set NODE_ENV=development&& tsx server/index.ts",
    "dev:unix": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production tsx server/index.ts",
    "start:win": "set NODE_ENV=production&& node dist/index.js",
    "start:unix": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit push",
    "db:migrate:safe": "tsx scripts/migrate-safe.ts",
    "db:dedupe": "tsx scripts/dedupe-threat-intel-direct.ts",
    "ti:ingest": "tsx scripts/trigger-ingestion.ts",
    "ti:dedupe:nightly": "node -e \"setInterval(()=>{},1<<30)\" & tsx scripts/dedupe-threat-intel-direct.ts",
    "migrate:orgid": "tsx server/migrate.ts",
    "campaign:launch:latest": "tsx scripts/launch-latest-campaign.ts",
    "campaign:inspect": "tsx scripts/inspect-results.ts",
    "import-data": "tsx scripts/import-data.ts",
    "setup": "npm run db:migrate:safe && npm run import-data && npm run db:dedupe",
    "kali": "NODE_ENV=development tsx server/index.ts",
    "==== SECURITY ====": "",
    "security:scan": "npm audit --audit-level=high",
    "security:secrets": "echo 'Scanning for secrets...' && grep -r 'password\\|secret\\|key\\|token' . --exclude-dir=node_modules || echo 'No secrets found'",
    "==== ACCESS CONTROL ====": "",
    "access:verify": "echo 'Access checks not required in unified workspace'",
    "access:check-permissions": "echo 'Unified workspace - no role-based workspace checks'",
    "==== TESTING WORKFLOWS ====": "",
    "test": "jest",
    "test:unit": "jest --testPathPattern='tests/unit' --coverage",
    "test:integration": "jest --testPathPattern='tests/integration'",
    "test:e2e": "playwright test",
    "test:security": "npm run security:scan",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov > coverage/lcov.info",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "==== UNIFIED APP WORKFLOWS ====": "",
    "frontend:dev": "vite --host 0.0.0.0 --port 5173",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview --host 0.0.0.0 --port 4173",
    "frontend:mock": "node scripts/frontend-mock-server.js",
    "frontend:full": "concurrently \"npm run frontend:mock\" \"npm run frontend:dev\"",
    "backend:dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "app:preview": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "app:full": "npm run dev",
    "manual:security": "npm run security:scan && npm run access:verify"
  },
